<script id="tpl_chat" type="text/html">
	<div id="quick-settings" class="row">
		<ul class="nav nav-pills pull-left">
			<li>
				<a id="chat-force-refresh" href="#"><span><i class="icon-retweet"></i> Refresh</span></a>
			</li>
			<li>
				<a href="#settings">Server IP: <span id="server-ip">{{ServerIP}}</span></a>
			</li>
		</ul>
		<ul class="nav nav-pills pull-right">
			<li>
				<a id="chat-toggle-options" href="#" class="place-right"><span><i class="icon-cogs"></i> Options & Archives</span></a>
			</li>
		</ul>
	</div>
	<div id="chat-quick-settings" class="row">
		<div class="page-header">
			<h1>Quick Settings</h1>
		</div>
		<div class="page-content">
			<div class="span5">
				<form id="historical-chat-form" class="form-horizontal">
					<fieldset>
						<legend>Load past history</legend>
                        <div class="control-group">
                            <label for="chat-date">Loading past logs will disable chat auto-refresh</label>
                            <input type="text" id="chat-date-input" name="chat-date" placeholder="Enter Date (MM/DD/YYYY)" />
                        </div>
						<div class="control-group">
							<button type="submit" id="load-archive" class="btn archive-btn">Load</button>
							<button type="button" id="save-archive" class="btn archive-btn">Save</button>
						</div>
						<iframe id="download-chat-iframe" src="{{iframe_url}}" style="display:none"></iframe>
					</fieldset>
				</form>
			</div>
			<div class="span7">
				<div id="chat-settings"></div>
			</div>
		</div>
	</div>
	<div class="row">
		<div class="page-header">
			<h1>Server Chat</h1>
		</div>
	</div>
	<div class="row">
		<div id="chat-contents" class="{{^message_filters.show_server_msgs}}hide-server-msgs{{/message_filters.show_server_msgs}} {{^message_filters.show_global_msgs}}hide-global-msgs{{/message_filters.show_global_msgs}} {{^message_filters.show_us_msgs}}hide-us-msgs{{/message_filters.show_us_msgs}} {{^message_filters.show_ru_msgs}}hide-ru-msgs{{/message_filters.show_ru_msgs}} {{^message_filters.show_squad_msgs}}hide-squad-msgs{{/message_filters.show_squad_msgs}}">
			<ul id="chat-contents-ul"></ul>
		</div>
	</div>
</script>

<script id="tpl_chat_messages" type="text/html">
	{{#.}}
		<li class="{{#Server}}server-message{{/Server}} team-{{MessageType}} {{#SquadName}}squad{{/SquadName}}">
