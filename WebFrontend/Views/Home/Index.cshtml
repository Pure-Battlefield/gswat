<script id="tpl_alert" type="text/html">
	<div class="container">
		<div class="row">
			<div class="alert alert-{{type}}">
				<button class="close" type="button" data-dismiss="alert">&times;</button>
				<strong>{{title}}</strong>
				<span>{{message}}</span>
			</div>
		</div>
	</div>
</script>

<script id="tpl_confirm_dialogue" type="text/html">
	<div id="modal" class="modal fade" tabindex="-1" role="dialog" aria-hidden="true">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                    <h4 id="modal-title">{{title}}</h4>
                </div>
                <div class="modal-body">
                    <p>{{message}}</p>
                </div>
                <div class="modal-footer">
                    <button class="btn btn-cancel" data-dismiss="modal" aria-hidden="true">Cancel</button>
                    <button class="btn btn-confirm btn-{{button}}">{{button-text}}</button>
                </div>
            </div>
        </div>
	</div>
</script>

